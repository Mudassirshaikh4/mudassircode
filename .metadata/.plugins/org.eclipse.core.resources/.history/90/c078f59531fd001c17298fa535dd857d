package statementlogic;

public class Primenumber {

	public static void main(String[] args) {
		int temp=0;
		for(int u=1;u<=100;u++)
		{	
		for (int g = 2; g <= u - 1; g++) {
			if (u % g == 0) {
				temp = temp + 1;
			}
		}
			if (temp == 0) {
				System.out.println(u);
			} 
			else
			{
				temp=0;
			}
		}
	}
}
